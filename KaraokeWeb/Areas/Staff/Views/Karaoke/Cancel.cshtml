@model KaraokeWeb.Models.EF.Room
@{
	ViewBag.Title = "Cancel";
	Layout = "~/Areas/Staff/Views/Shared/_Layouts.cshtml";
}
@section header{

}
<div class="sign-up">
	@using (Html.BeginForm())
	{
		@Html.AntiForgeryToken()
		<h3 class="text-uppercase">Trạng thái</h3>

		<h6></h6>
		@Html.ValidationSummary(true, "", new { @class = "text-danger" })
		@Html.HiddenFor(model => model.room_id)
		<div class="form-group">
			<div class="sign-u">
				<div class="sign-up1">
					<h4>Trạng thái* :</h4>
				</div>
				<div class="sign-up2">
					@Html.TextBoxFor(model => model.r_status, new { @class = "form-control" })
					@Html.ValidationMessageFor(model => model.r_status, "", new { @class = "text-danger" })
				</div>
				<div class="clearfix"> </div>
			</div>
		</div>

		<div class="sub_home">
			<div class="sub_home_left">
				<button type="submit">Cập nhật trạng thái</button>
			</div>
			<div class="sub_home_right">
				@Html.ActionLink("Quay lại trang chủ", "ShowRoom", "Karaoke")
			</div>
			<div class="clearfix"> </div>
		</div>
	}
</div>

